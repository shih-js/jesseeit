{
	"name": "jesseeit",
	"description": "A chrome extension that allows users to quickly speed read by holding down the 'c' key and clicking any block of text on screen.",
	"homepage": "https://jesseeit.com",
	"baseUrl": "https://jesseeit.com",
	"version": "1.0.0",
	"private": true,
	"main": "src/index.js",
	"dependencies": {
		"parcel-bundler": "^1.12.4",
		"react": "^16.12.0",
		"react-dom": "^16.12.0",
		"styled-components": "^4.4.1"
	},
	"scripts": {
		"build": "parcel build src/index.js -d build/ -o main.js",
		"dev": "parcel watch src/index.js -d build/ -o main.js",
		"watch": "parcel watch src/index.js -d build/ -o main.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/shih-js/jesseeit.git"
	},
	"author": "Jeff Shih",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/shih-js/jesseeit/issues"
	},
	"keywords": [
		"chrome",
		"extension",
		"speed",
		"read",
		"flash",
		"words"
	],
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
}
